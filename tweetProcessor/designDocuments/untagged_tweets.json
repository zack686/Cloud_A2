{
    "_id": "_design/untagged_tweets",
    "views": {
        "untagged_geo_tweets": {
            "map": "function (doc) {if (doc.geo) {if (doc.id_str) { emit(doc.id_str, 1); } else if (doc.id) { emit(doc.id.toString(), 1); }}}",
            "reduce": "_sum"
        }
    },
    "language": "javascript"
}